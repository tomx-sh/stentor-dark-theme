	"tokenColors": [

		// Global foreground
		{
			"name": "source.ts",
			"scope": "source.ts, constant.numeric, meta.rule.lexer.antlr",
			"settings": {
			  "foreground": "#EED097"
			}

		},

		// Keywords
		// Maybe use "storage"
		{
			"name": "import statements",
			"scope": "keyword.control, storage.type, storage.modifier.ts, support.class.promise, storage.modifier.async, entity.name.tag.wildcard",
			"settings": {
			  "foreground": "#eb7eb3"
			}
		},

		  // Strings
		  {
			"name": "string",
			"scope": "string, punctuation.definition.string.begin,punctuation.definition.string.end",
			"settings": {
			  "foreground": "#E9A692" //#e88a7e
			}
		},
		  
		// Ponctuation, = 
		{
			"name": "Ponctuation",
			"scope": "punctuation, keyword.operator.type.annotation, keyword.operator.assignment, meta.brace.round, meta.brace.square, meta.rule.parser.antlr",
			"settings": {
				"foreground": "#424a59"
			}
		},

		// Nom des variables appelées
		{
			"name": "Variables appelées",
			"scope": "variable.other, variable.other.object.tsx, storage.type.function.antlr, constant.other.antlr",
			"settings": {
				"foreground": "#d0d9df"
			}
		},
		/*
		{
			"name": "Propriétés des variables",
			"scope": "variable.other.property.ts, variable.other.object.property.ts, variable.other.property.tsx",
			"settings": {
				"foreground": "#EED097" // #d0d9df
			}
		},
		*/
		
		

		// Noms des types
		{
			"name": "Noms des types",
			"scope": "entity.name.type, entity.other.inherited-class.ts, support.type, keyword.other.unit",
			"settings": {
				"foreground": "#6DDED5",
				"fontStyle": "italic"
			}
		},

		{
			"name": "Déclaration des types",
			"scope": "entity.name.type.interface, entity.name.type.alias",
			"settings": {
				"fontStyle": "italic bold"
			}
		},

		// Fonctions
		{
			"name": "Noms des fonctions",
			"scope": "entity.name.function, variable.scss, entity.name.rule.antlr",
			"settings": {
				"foreground": "#7198ee"
			}
		},

		{
			"name": "Déclaration des fonctions",
			"scope": "meta.definition.function, entity.name.rule.antlr, storage.type.var.antlr",
			"settings": {
				"fontStyle": "bold"
			}
		},

		// Propriétés des objets et paramètres des fonctions
		{
			"name": "Noms des propriétés des objets",
			"scope": "variable.object.property, meta.object-literal.key, variable.parameter, entity.other.attribute-name.tsx, entity.other.attribute-name.html, support.type.property-name.css",
			"settings": {
				"foreground": "#8a9fb4",
				"fontStyle": ""
			}
		},

		// Operateurs sauf =
		{
			"name": "operator logical",
			"scope": "keyword.operator, punctuation.accessor.optional",
			"settings": {
				"foreground": "#99e09e"
			}
		},


		// Commentaires
		{
			"name": "Comments",
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#49545f",
				"fontStyle": "italic"
			}
		},


		// TSX
		{
			"name": "Nom des composants",
			"scope": "support.class.component.tsx, entity.other.attribute-name.id.css, entity.other.attribute-name.class.css, storage.type.var.antlr",
			"settings": {
				"foreground": "#C792E9",
			}
		},

		{
			"name": "Nom des tags HTML par défaut",
			"scope": "entity.name.tag.html, entity.name.tag.tsx, entity.name.tag.css",
			"settings": {
				"foreground": "#A092E9",
			}
		},

		



		  
	]
}
